<%# TODO: company details page %>
<h1><%= @company["name"]%></h1>
<h2><%= @company["city"]%>, <%= @company["state"]%></h2>
<a href="/companies">All Companies</a>
<h3> Contacts </h3>
<% %>
<ul>
    <%for contact in @contacts %>
     <li>
        <%= contact["first_name"]%>
        <%= contact["last_name"]%>
     </li>
    <%end%>
</ul>

<p>
    <a href="/contacts/new?company_id=<%= @company["id"]%>">Create a contact at <%= @company["name"]%></a>
</p>